<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
    	<typeAlias alias="UserModel" type="com.spring.Model.User" />
    		<typeAlias alias="RegisterFormModel" type="com.spring.Model.RegisterForm" />
    	<typeAlias alias="ProductModel" type="com.spring.Model.Product" />
    	<typeAlias alias="Login" type="com.spring.Model.Login" />
      </typeAliases>
    
    <mappers>
       	<mapper resource="com/example/security/model/User-MyBatis.xml" />
       		<mapper resource="com/example/security/model/Message-MyBatis.xml" />
       		<mapper resource="com/example/security/model/Product-MyBatis.xml" />
       		<mapper resource="com/example/security/model/Login-MyBatis.xml" />
    </mappers>   
    
</configuration>                            